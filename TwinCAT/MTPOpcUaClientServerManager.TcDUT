	Reader_QC_NamespaceIndex						: UINT			:= 0;			//Namespace index of the quality code of the currently selected reader
	Reader_QC_Identifier							: STRING		:= '';			//Identifier of the quality code of the currently selected reader
	Reader_QC_IdentifierType						: UDINT			:= 0;			//Identifier type of the quality code of the currently selected reader
	WriterViewSel									: BOOL			:= FALSE;		//Selection to view prepared configuration (false) or active configuration (true)
	WriterViewCur									: BOOL			:= FALSE;		//Currently selected view: false = prepared, true = active
	WriterCountInUse								: UINT			:= 0;			//Number of writers in use
	WriterCountError								: UINT			:= 0;			//Number of writers with failures
	WriterIndexSel									: UINT			:= 0;			//Index of the desired writer configuration to be shown
	WriterIndexMax									: UINT			:= 0;			//Maximum index for writer configuration
	WriterIndexCur									: UINT			:= 0;			//Index of the currently selected writer configuration
