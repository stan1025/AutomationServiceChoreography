	Writer_RestoreDefault							: BOOL			:= FALSE;		//Restore the default configuration of the currently selected writer
	Writer_Reset									: BOOL			:= FALSE;		//Reset the writer
	Writer_ConnectionIndex							: INT			:= 0;			//Connection index the currently selected writer should use
	Writer_OutputIndex								: UINT			:= 0;			//Indicates the index of the Output List the writer refers to
	Writer_DataTypeSel								: BYTE			:= 16#0;		//Data type of the currently selected writer
	Writer_Timeout									: TIME			:= T#0MS;		//Timeout for the used OPC UA operations
	Writer_MaxTryCount								: BYTE			:= 16#0;		//Number of tries for an OPC UA operation until the writer transitions : INTo the error state
	Writer_CycleSel									: TIME			:= T#0MS;		//Target cycle for the write operation
	Writer_CycleCur									: TIME			:= T#0MS;		//Actual write cycle
	Writer_Error									: BOOL			:= FALSE;		//True, if the writer is in the error state
	Writer_Status									: UDINT			:= 0;			//Status of the Writer (e. g. status codes of OPC UA operations in case of an error)
	Writer_Value_NamespaceIndex						: UINT			:= 0;			//Namespace index of the value of the currently selected writer
	Writer_Value_Identifier							: STRING		:= '';			//Identifier of the value of the currently selected writer
	Writer_Value_IdentifierType						: UDINT			:= 0;			//Identifier type of the value of the currently selected writer
END_STRUCT
END_TYPE
]]></Declaration>
