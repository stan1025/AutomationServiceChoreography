	ReaderViewSel									: BOOL			:= FALSE;		//Selection to view prepared configuration (false) or active configuration (true)
	ReaderViewCur									: BOOL			:= FALSE;		//Currently selected view: false = prepared, true = active
	ReaderCountInUse								: UINT			:= 0;			//Number of readers in use
	ReaderCountError								: UINT			:= 0;			//Number of readers with failures
	ReaderIndexSel									: UINT			:= 0;			//Index of the desired reader configuration to be shown
