<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4026.4">
  <DUT Name="MTPOpcUaClientServerManager" Id="{e5be6510-64d3-4c6d-97d8-7b84143013ec}">
    <Declaration><![CDATA[{attribute 'OPC.UA.DA' := '1'}
{attribute 'OPC.UA.DA.StructuredType' := '1'}
TYPE MTPOpcUaClientServerManager :
STRUCT
	WQC												: BYTE			:= 16#0;		//Worst Quality Code
	ConnectionViewSel								: BOOL			:= FALSE;		//Selection to view prepared configuration (false) or active configuration (true)
	ConnectionViewCur								: BOOL			:= FALSE;		//Currently selected view: false = prepared, true = active
	ConnectionIndexSel								: BYTE			:= 16#0;		//Index of the desired connection configuration to be shown
	ConnectionIndexMax								: BYTE			:= 16#0;		//Maximum index for connection configuration
	ConnectionIndexCur								: BYTE			:= 16#0;		//Index of the currently selected connection configuration
	ConnectionCountActive							: BYTE			:= 16#0;		//Number of active connections
	ConnectionCountInactive							: BYTE			:= 16#0;		//Number of inactive but configured connections
	ConnectionCountError							: BYTE			:= 16#0;		//Number of failed connections
