	Connection_RestoreDefault						: BOOL			:= FALSE;		//Restore Default configuration of the currently selected connection
	Connection_ConnectEn							: BOOL			:= FALSE;		//Connect the currently selected connection
	Connection_Connect								: BOOL			:= FALSE;		//Connect the currently selected connection
