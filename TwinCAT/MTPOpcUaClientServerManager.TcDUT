	Reader_DataTypeSel								: BYTE			:= 16#0;		//Data type of the currently selected reader
	Reader_Timeout									: TIME			:= T#0MS;		//Timeout for the used OPC UA operations
	Reader_MaxTryCount								: BYTE			:= 16#0;		//Number of tries for an OPC UA operation until the Reader transitions : INTo the error state
	Reader_CycleSel									: TIME			:= T#0MS;		//Target cycle for the read operation
	Reader_CycleCur									: TIME			:= T#0MS;		//Actual read cycle
