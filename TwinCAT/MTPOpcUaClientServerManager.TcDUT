	Connection_SessionInfo_MonitorConnection		: TIME			:= T#0MS;		//: INTerval time to check the connection
	Connection_SessionInfo_LocaleID_1				: STRING[6]		:= '';			//Optional language and regional identifier acc. to RFC 3066. 0 = no or unknown LocaleID.
	Connection_SessionInfo_LocaleID_2				: STRING[6]		:= '';			//Optional language and regional identifier acc. to RFC 3066. 0 = no or unknown LocaleID.
	Connection_SessionInfo_LocaleID_3				: STRING[6]		:= '';			//Optional language and regional identifier acc. to RFC 3066. 0 = no or unknown LocaleID.
	Connection_SessionInfo_LocaleID_4				: STRING[6]		:= '';			//Optional language and regional identifier acc. to RFC 3066. 0 = no or unknown LocaleID.
	Connection_SessionInfo_LocaleID_5				: STRING[6]		:= '';			//Optional language and regional identifier acc. to RFC 3066. 0 = no or unknown LocaleID.
